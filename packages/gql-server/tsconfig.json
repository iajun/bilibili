    "outDir": "dist",
    "strict": true,
    "target": "es6",
    "lib": ["es6", "DOM", "DOM.Iterable"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "removeComments": true,
